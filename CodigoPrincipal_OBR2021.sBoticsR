tarefa rampa{
	se (cor(2) == "BRANCO" e cor(4) == "BRANCO") entao {
		acenderled("PRETO")
  		frente(230)
	}
   	se (cor(2) == "BRANCO" e cor(4) == "PRETO") entao {
  		esquerda(1000)
  	}   
	se (cor(2) == "PRETO" e cor(4) == "BRANCO") entao {
		direita(1000)
	}
}

tarefa seguirLinha{
