}

tarefa desviarObstaculo{
	rotacionar(1000, 90) 					#rotaciona 90 graus pra direita
	frente(200)
	esperar(700) 								#segue em frente pro 1 segundo
	rotacionar(500, 0-90) 					#rotaciona 90 graus pra esquerda
	#esperar(3000)
	frente(200)
	esperar(2200)                    	#segue em frente por 2.1 segundos
	rotacionar(500, 0-90)            	#rotaciona 90 graus pra esquerda
	frente(200)
	esperar(700)                   	#segue em frente por 1 segundo
	rotacionar(1000, 90) 					#aqui ele ja vai estar na linha e vai rotacionar pra direita, nisso ele vai ajustar
}

tarefa  buscarVitima{
	tras(190)
	esperar(100)
	abrir(1000)
	baixar(600)
		enquanto(temvitima() != verdadeiro) farei{
			frente(200)
		}
		esperar(300)
		se(temvitima() != falso) entao{
			levantar(600)
			fechar(1000)
		}
}

